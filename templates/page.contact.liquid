<script>
    (function(){
        $(document).ready(function () {

            var classes = {
                inputError: "input--error"
            };

            var $requiredField = $('#ContactFrom *[required]');
            var $form = $('#ContactFrom');
            var $submit = $('.form-action input[type="submit"]');

            $form.on('change paste', 'input, select, textarea', function() {
                var isValid = true;

                $requiredField.each(function( index, element ) {
                    console.log($(element).val());
                    if (!$(element).val()) {
                        isValid = false;
                        return;
                    }
                });

                if(isValid) {
                    $submit.removeAttr('disabled');
                } else {
                    $submit.attr('disabled');
                }
            });
        })
    })();
</script>
