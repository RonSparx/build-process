{% schema %}
  {
    "name": "Alpha Three",
    "settings": [
{
	  "type":"header",
	  "content":"Youtube section"
},
{
	"type":"html",
	"id":"video",
	"label":"Video link",
	"info":"Youtube Embed video URL"
},
{
	"type":"text",
	"id":"heading",
	"label":"Heading",
	"default":"INTRODUCING"
},
{
	"type":"text",
	"id":"sub_heading",
	"label":"Sub Heading",
	"default":"tumi+ travel Accessories "
}, 
{
	"type":"richtext",
	"id":"content",
	"label":"Content",
	"default":"<p>Ready for life on the go, modular TUMI+ Travel Accessories bring even more function, purpose and adaptability to your bag..</p>"
},
{
	"type":"text",
	"id":"btn",
	"label":"Button Name",
	"default":"Shop the collection"
},
{
	"type":"url",
	"id":"link",
	"label":"Button URL"
},
{
	  "type":"header",
	  "content":"Collection section"
},
{
	"type":"text",
	"id":"collection_name",
	"label":"Collection Name",
	"default":"Lando's TUMI+ Travel Accessories"
},
{
	"type":"collection",
	"id":"product_col",
	"label":"Select Collection"
}
],
"blocks":[
	{
	"name":"block",
	"type":"column",
	"settings":[

  {
  "type":"image_picker",
  "id":"img",
  "label":"Select Image"
  },
  {
  "type":"text",
  "id":"heading",
  "label":"Heading"
  },
  {
  "type":"text",
  "id":"btn",
  "label":"Button name"
  },
  {
  "type":"url",
  "id":"link",
  "label":"Categories URL"
  }
]
}
]
  }
{% endschema %}

