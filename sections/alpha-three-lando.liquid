  <section  id="aplhathree">
    <div class="page-width">
      <div class="backpack-wrap">
        <div class="backpack-video">
          <div class="video-container">
            {% if section.settings.video != blank %}
              <iframe width="100%"  src="https://www.youtube.com/embed/{{section.settings.video}}" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
            {% else %}
            <iframe width="100%" src="https://www.youtube.com/embed/z3nQd-uw8i4" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
         	{% endif %}
          </div>
        </div>

        <div class="backpack-con travel-con">
          <span class="subheading">{{section.settings.heading}}</span>
          <h2>{{section.settings.sub_heading}} </h2>
          {{section.settings.content}}
          
          {% if section.settings.btn != blank %}
          <a href="{{section.settings.link | default:'#' }}" class="btn-transparent">
            <span class="ben-text">{{section.settings.btn}}</span>
            <i class="fas fa-caret-right" aria-hidden="true"></i>
          </a>
          {% endif %}
        </div>

      </div>
    </div>


    <div class="travel-sliderWrap">
      <div class="discover-sliderWrap alpha-slider">
        <div class="header-block">
          <h2 class="block_title">
            {{section.settings.collection_name}}
          </h2>
        </div>

        
        <!-- -->
        
        <div class="travel-sliderWrap explore-slider">

        <div class="custom-row explore-row">
          {% if section.settings.product_col != blank %}
          {% for product in collections[section.settings.product_col].products %}
          	{% assign image  = product.featured_media.preview_image %}
          <div class="explore-item">
            <div class="discover_img">
              <a href="{{product.url}}">
              <img src="{{image | img_url: '300x'}}" alt="{{product.title}}" />
              {% if product.tags == 'new' %}
              <span class="product-tag">
                New
              </span>
              {% endif %}
              </a>
            </div>
            <div class="travel-sliderItem" style="max-width:100%; ">
            <div class="discover_desc">
               <a href="{{product.url}}">
              <h3>{{product.title}}</h3>
              <span class="product_subtitle">{{product.type}}</span>
              <span class="product_price"> {{product.price | money }}</span>
              </a>
            </div>
            </div>
          </div>
          	
          {% endfor %}
          
         {% else %}
          
          {% for i in (1..4) %}
           <div class="explore-item">
            <a href="#">
             <img src="https://cdn.shopify.com/s/files/1/0272/4233/2203/files/03-AlphaBravo-NavigationBackpack-Desktop.jpg" alt="b1">
            <div class="explore-content">
              <h4>Navigation Backpack</h4>
              <span>alpha bravo</span>
            </div>
             </a>
          </div>
          {% endfor %} 
 {% endif %}
        </div>
        </div>
        
        
        <!-- -->
        
        
        
