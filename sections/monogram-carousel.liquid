  <section class="container-full carousel-sec">
    <h3 class="heading-block wow fadeInUp">
       {{ section.settings.heading }}
    </h3>

    <div class="personalization-carousel">
      
      {% if section.blocks != blank %}
      
      {% for block in section.blocks  %}
            {% if block.settings.allignment == 'right' %}
      		<div class="carousel-item">
        <div class="carousel-row rev_block">             
          <div class="carousel-col">            
            <div class="image-carousel">
              {% if block.settings.img_one != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_one | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_two != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_two | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_three != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_three | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_four != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_four | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_five != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_five | img_url:'480x'}}"  />
              </div>
              {% endif %}
            </div>
          </div>          
          <div class="carousel-col carousel-text">
             {{block.settings.content}}
          </div>         
           
        </div>
      </div>
      
      {% else %}
      <div class="carousel-item">
        <div class="carousel-row">             
          <div class="carousel-col">            
            <div class="image-carousel">
              {% if block.settings.img_one != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_one | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_two != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_two | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_three != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_three | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_four != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_four | img_url:'480x'}}"  />
              </div>
              {% endif %}
               {% if block.settings.img_five != blank  %}
              <div class="image-citems">
                <img src="{{block.settings.img_five | img_url:'480x'}}"  />
              </div>
              {% endif %}
            </div>
          </div>          
          <div class="carousel-col carousel-text">
             {{block.settings.content}}
          </div>         
           
        </div>
      </div>
      
      {% endif %}
      
      {% endfor %}
      {% else %}
      <div class="carousel-item">
        <div class="carousel-row">
          <div class="carousel-col">
            <div class="image-carousel">
              <div class="image-citems">
                <img src="https://cdn.shopify.com/s/files/1/0272/4233/2203/files/carousel-1-black.jpg?v=1646738763" alt="img" />
              </div>
              <div class="image-citems">
                <img src="https://cdn.shopify.com/s/files/1/0272/4233/2203/files/carousel-1-red.jpg?v=1646738764" alt="img" />
              </div>
               <div class="image-citems">
                <img src="https://cdn.shopify.com/s/files/1/0272/4233/2203/files/carousel-1-blue.jpg?v=1646823132" alt="img" />
              </div>
              
            </div>
          </div>
          <div class="carousel-col carousel-text">
            <p>
              TUMI Accent Kits offers a wide range of unique color accents for your monogram patch, luggage tag and zippers. Adding a unique touch of personality to your favorite TUMI products, making you stand out from the crowd and perfecting your journey.
            </p>
          </div>
        </div>
      </div>

      <div class="carousel-item">
        <div class="carousel-row rev_block">
           <div class="carousel-col">
            <div class="image-carousel">
              <div class="image-citems">
                <img src="https://cdn.shopify.com/s/files/1/0272/4233/2203/files/carousel-2-blue.jpg?v=1646739121" alt="img" />
              </div>
              <div class="image-citems">
                <img src="https://cdn.shopify.com/s/files/1/0272/4233/2203/files/carousel-2-red.jpg?v=1646739121" alt="img" />
              </div>
              <div class="image-citems">
                <img src="https://cdn.shopify.com/s/files/1/0272/4233/2203/files/carousel-2-white.jpg?v=1646823257" alt="img" />
              </div>
              
              
            </div>
          </div>
          <div class="carousel-col carousel-text">
            <p>
              TUMI Accent Kits offers a wide range of unique color accents for your monogram patch,
              luggage tag and zippers. Adding a unique touch of personality to your favorite TUMI
              products, making you stand out from the crowd and perfecting your journey.
            </p>
          </div>
        </div>
      </div>
 {% endif %}

    </div>


  </section>
<style>
  @media(min-width:992px) {
  	.carousel-row.rev_block {
    flex-direction: row-reverse;
}
  }
</style>
{% schema %}
  {
    "name": "Monogram Carousel",
    "settings": [
{
"type":"html",
"id":"heading",
"label":"Heading",
"default":"<span class='text-bold'> STYLE YOUR TUMI </span><span class='text-light'> - Accent Kits </span>"
}
],
"blocks":[
{
	"type":"carousel",
	"name":"carousel",
	"settings":[
{
"type":"richtext",
"id":"content",
"label":"Content"
},
{
"type": "select",
"id": "allignment",
"label": "Image align",
"default": "left",
"options": [
{
"value": "left",
"label": "Left"
},
{
"value": "right",
"label": "Right"
}
]
},
{
"type":"image_picker",
"id":"img_one",
"label":"Slide Image one"
},
{
"type":"image_picker",
"id":"img_two",
"label":"Slide Image two"
},
{
"type":"image_picker",
"id":"img_three",
"label":"Slide Image three"
},
{
"type":"image_picker",
"id":"img_four",
"label":"Slide Image four"
},
{
"type":"image_picker",
"id":"img_five",
"label":"Slide Image five"
}
]
}
]
  }
{% endschema %}

{% stylesheet %}
{% endstylesheet %}

{% javascript %}
{% endjavascript %}
