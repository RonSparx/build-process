{% if section.blocks != blank %}
<div class="year-Blocks">
  <ul>
    {% for block in section.blocks %}
    <li>
      <div class="inner-wrper">
        <h2>{{block.settings.number}}</h2>
        <p>{{block.settings.heading}}</p>
        <p>{{block.settings.content}}</p>
        <div class="yearBlockImage">
          {% if block.settings.img != blank %}
         <img src="{{block.settings.img | img_url:'600x'}}" />
          {% else %}
          
          {% endif %}
        </div>
        </div>
    </li>
   {% endfor %}
  </ul>
</div>
